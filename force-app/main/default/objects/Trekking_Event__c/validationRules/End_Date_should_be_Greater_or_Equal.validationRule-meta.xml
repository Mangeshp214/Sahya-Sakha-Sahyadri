<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_should_be_Greater_or_Equal</fullName>
    <active>true</active>
    <errorConditionFormula>Start_time__c   &gt;=  End_Time__c</errorConditionFormula>
    <errorDisplayField>End_Time__c</errorDisplayField>
    <errorMessage>End Date should be greater than or equal to Start Date.</errorMessage>
</ValidationRule>
